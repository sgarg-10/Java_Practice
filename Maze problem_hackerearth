#include <stdio.h>

int main(){
    char s1[200];
    int x,y;
    x=0;
    y=0;
    scanf("%s",&s1);
    int len=strlen(s1);
    for(int i=0;i<len;i++)
    {
        if(s1[i]=='L')
        x=x-1;
        else if(s1[i]=='R')
        x=x+1;
        else if(s1[i]=='U')
        y=y+1;
        else
        y=y-1;
        
    }
    printf("%d %d ",x,y);
}
